staxel.village.dialogue.Shy.line:10002200=
//TODO [Reference] 'I'd like to play with some of the animals.'
staxel.village.dialogue.Shy.line:10002300=
//TODO [Reference] 'Oh... only if that's okay...'
staxel.village.dialogue.Shy.line:10002400=
